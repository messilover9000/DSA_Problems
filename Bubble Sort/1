#include <stdio.h>

// Corrected function declaration with return type
void bubble_sort(int a[], int N)
{
    int i, j, temp;

    printf("Before Sorting:\n");
    for (i = 0; i < N; i++)
    {
        printf("%d ", a[i]);
    }
    printf("\n");

    // Bubble Sort Algorithm
    for (i = 0; i < N; i++)
    {
        for (j = 0; j < N - 1 - i; j++)
        {
            if (a[j] > a[j + 1])
            {
                temp = a[j];
                a[j] = a[j + 1];
                a[j + 1] = temp;
            }
        }
    }

    printf("After Sorting:\n");
    for (i = 0; i < N; i++)
    {
        printf("%d ", a[i]);
    }
    printf("\n"); // Added newline for better output formatting
}

int main()
{
    int a[10], i, N;

    printf("Enter a range: ");
    scanf("%d", &N);
    printf("Enter %d numbers: ", N);
    for (i = 0; i < N; i++)
    {
        scanf("%d", &a[i]);
    }
    bubble_sort(a, N);
    return 0; // Added return statement for main
}